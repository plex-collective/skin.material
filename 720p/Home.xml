<?xml version="1.0"?>
<window id="0">
	<defaultcontrol always="true">300</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<include condition="!Skin.HasSetting(SHOW_GRID) + !Skin.HasSetting(SHOW_DEBUG_INFO)">SlideShowHome</include>
		<include>BGDimmingOverlay</include>
		<include>Header</include>

		<include condition="Skin.String(HOME_VIEW,Material) | IsEmpty(Skin.String(HOME_VIEW))">HomeMenu</include>
		<include condition="Skin.String(HOME_VIEW,Urban)">HomeUrbanMenu</include>

		<include>GlobalIncludes</include>
		<include>Clock</include>
	</controls>
</window>
